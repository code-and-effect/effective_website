- flash.each do |key, value|
  .container
    .row
      .col-xs-12
        %div{class: "alert alert-dismissible alert-#{key}", role: 'alert'}
          %button.close{type: 'button', aria: { label: 'Close' }, data: { dismiss: 'alert' }}
            %span{'aria-hidden': true} &times;
            %span.sr-only Close
          = value.to_s.html_safe
